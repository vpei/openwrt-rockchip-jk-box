
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/fe2c0000.mmc/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option channel '1'
	option band '2g'
	option cell_density '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'H-WiFi'
	option encryption 'psk-mixed'
	option key '23456701'
	option macaddr 'D6:D5:5C:25:5A:11'

