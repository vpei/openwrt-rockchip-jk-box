
config system
	option ttylogin '0'
	option urandom_seed '0'
	option log_proto 'udp'
	option conloglevel '1'
	option cronloglevel '9'
	option log_size '8'
	option hostname 'TVBOX-C'
	option description 'C-TVBOX-NET-Description'
	option notes 'C-TVBOX-NET-Notes'
	option zonename 'Asia/Hong Kong'
	option timezone 'HKT-8'
	option zram_comp_algo 'lzo'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config led
	option name 'red'
	option sysfs 'red:work'
	option trigger 'netdev'
	option dev 'eth0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'white'
	option sysfs 'mmc0::'
	option trigger 'default-on'

